<form action="<%= update_listing_path %>" method="POST" enctype="multipart/form-data"  >
<input type="hidden" name="_method" value="patch" />
<input type="hidden" value="<%= form_authenticity_token %>" name="authenticity_token" />
  <label for="title">Title</label>
  <input type="text" name="title" id="title"/>

  <label for="address">Address</label>
  <input type="text" name="address" name="address" id="location"/>

  <label>Description</label>
  <input type="textarea" name="description" id="description" />

  <label for="cost">Cost</label>
  <input type="integer" name="cost" id="cost" />

  <label for = "uploaded_images"> Upload Images </label>
  <input type = "file" name = "uploaded_images[]" id = "uploaded_images" multiple/>

  <input type="submit" value="Edit" />
</form>